{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "OpenTelemetry.UseOtlpExporterIssue-RawEnvVar": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "http://www.google.com"
      }
    },
    "OpenTelemetry.UseOtlpExporterIssue-PrefixedEnvVarToConfiguration": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development",
        "MyCustomPrefix_OTEL_EXPORTER_OTLP_ENDPOINT": "http://www.google.com"
      }
    }
  }
}
